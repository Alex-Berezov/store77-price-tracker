{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "description": "NestJS Backend API for Store77 Price Tracker",
  "scripts": {
    "prebuild": "rimraf dist tsconfig.build.tsbuildinfo",
    "build": "tsc -p tsconfig.build.json",
    "dev": "npm run build && concurrently \"tsc -p tsconfig.build.json -w\" \"nodemon dist/main.js\"",
    "start": "node dist/main.js",
    "start:dev": "npm run build && node dist/main.js",
    "start:debug": "npm run build && node --inspect dist/main.js",
    "start:prod": "node dist/main.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/client": "5.22.0",
    "axios": "^1.13.4",
    "cache-manager": "^7.2.8",
    "cache-manager-ioredis-yet": "^2.1.2",
    "cheerio": "^1.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "ioredis": "^5.9.2",
    "joi": "^17.12.0",
    "playwright": "^1.58.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^10.3.0",
    "@types/cheerio": "^1.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prisma": "5.22.0",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
